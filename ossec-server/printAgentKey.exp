#!/bin/expect

set timeout -1
set SERVER_ID [lindex $argv 0];
spawn /var/ossec/bin/manage_agents

expect "Choose your action: A,E,L,R or Q:"
sleep 2
send -- "E\r"

expect "Provide the ID of the agent to extract the key "
sleep 2
send -- "$SERVER_ID\r"

expect "** Press ENTER to return to the main menu."
sleep 2
send -- "\r"

expect "Choose your action: A,E,L,R or Q:"
sleep 2
send -- "Q\r"
